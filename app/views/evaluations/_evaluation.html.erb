<dl id="<%= dom_id evaluation %>" class="row">
  <dt class="col-sm-3">Course</dt>
  <dd class="col-sm-9"><%= link_to evaluation.course.title, evaluation.course %></dd>

  <dt class="col-sm-3">Teacher</dt>
  <dd class="col-sm-9"><%= evaluation.teacher.full_name %></dd>

  <dt class="col-sm-3">Student</dt>
  <dd class="col-sm-9"><%= evaluation.student.full_name %></dd>

  <dt class="col-sm-3">Grade</dt>
  <dd class="col-sm-9"><%= number_with_precision(evaluation.grade, precision: 2) %></dd>

  <dt class="col-sm-3">Created at</dt>
  <dd class="col-sm-9"><%= evaluation.created_at %></dd>

  <%= link_to 'Show', evaluation %>
</dl>
